<?xml version="1.0" encoding="UTF-8"?>

<GUILayout >
	<Window Type="TaharezLook/FrameWindow" Name="TeleportFrame" >
	    <Property Name="Text" Value="Select a location to teleport" />	
	    <Property Name="TitlebarFont" Value="Commonwealth-10" />
	    <Property Name="UnifiedMaxSize" Value="{{1,0},{1,0}}" />
	    <Property Name="TitlebarEnabled" Value="True" />
	    <Property Name="UnifiedAreaRect" Value="{{0,10},{0.2,0},{0.1,200},{0.1,350}}" /> 
	    <Window Type="TaharezLook/Listbox" Name="TeleportList" >
		<Property Name="UnifiedMaxSize" Value="{{1,0},{1,0}}" />
		<Property Name="UnifiedAreaRect" Value="{{0,10},{0,0},{1,-10},{1,-45}}" />
	    </Window>
       
         <Window Type="TaharezLook/Button" Name="TeleporGoButton">
            <Property Name="Text" Value="GO" />
                <Property Name="UnifiedAreaRect" Value="{{0.5,-90},{1,-35},{0.5,-10},{1,-10}}" />
        </Window>  
        
        <Window Type="TaharezLook/Button" Name="TeleportCancelButton">
            <Property Name="Text" Value="Cancel" />
                <Property Name="UnifiedAreaRect" Value="{{0.5,10},{1,-35},{0.5,90},{1,-10}}" />
        </Window> 	    
	    
	</Window>
</GUILayout>
