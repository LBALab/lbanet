			<Property Name="UnifiedAreaRect" Value="{{0,10},{0,25},{1,-10},{0,55}}" />
		    </Window>   

		 <Window Type="TaharezLook/Button" Name="ChooseNumberBoxFrame/bOk">
		    <Property Name="Text" Value="OK" />
