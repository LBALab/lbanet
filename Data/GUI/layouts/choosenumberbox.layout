<?xml version="1.0" ?>
<GUILayout>
    <Window Type="DefaultWindow" Name="ChooseNumberBoxRoot" >
        <Property Name="UnifiedMaxSize" Value="{{1,0},{1,0}}" />
        <Property Name="UnifiedAreaRect" Value="{{0,0},{0,0},{1,0},{1,0}}" />
        
        
	    <Window Type="TaharezLook/FrameWindow" Name="ChooseNumberBoxFrame" >
		<Property Name="Text" Value="Please specify the item number" />
		<Property Name="TitlebarFont" Value="Commonwealth-10" />
		<Property Name="UnifiedMaxSize" Value="{{1,0},{1,0}}" />
		<Property Name="UnifiedMinSize" Value="{{0,128},{0,128}}" />
		<Property Name="TitlebarEnabled" Value="True" />
		<Property Name="UnifiedAreaRect" Value="{{0.3,0},{0.3,0},{0.3,300},{0.3,60}}" />

		    <Window Type="TaharezLook/Spinner" Name="ChooseNumberBoxFrame/spinner" >
			<Property Name="StepSize" Value="1" />
